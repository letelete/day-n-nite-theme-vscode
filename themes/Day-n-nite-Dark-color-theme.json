    "tokenColors": [
        {
            "scope": [
                "comment",
                "string.quoted.docstring"
            ],
            "settings": {
                "foreground": "#525252"
            }
        },
        {
            "scope": [
                "string"
            ],
            "settings": {
                "foreground": "#737373"
            }
